{
  "name": "test2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "clean:ngc": "find src -name '*.ngfactory.ts' -type f -delete && find src -name '*.js' -type f -delete && find src -name '*.js.map' -type f -delete && find src -name '*.shim.ts' -type f -delete",
    "serve": "http-server . -p 5557",
    "ngc": "ngc -p tsconfig.aot.json",
    "build-static": "gulp build",
    "rollup": "rollup -f iife -c rollup.config.js",
    "rollup:aot": "rollup -f iife -c rollup.config.aot.js",
    "es5": "tsc --target es5 --allowJs build/bundle.es2015.js --out build/bundle.js",
    "minify": "uglifyjs build/bundle.js --screw-ie8 --compress --mangle --output build/bundle.min.js",
    "build": "npm run clean && npm run rollup && npm run es5 && npm run minify",
    "build:aot": "npm run clean && npm run build-static && npm run ngc && npm run rollup:aot && npm run clean:ngc && npm run es5 && npm run minify"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/compiler-cli": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/platform-server": "2.1.0",
    "@angular/router": "^3.3.0",
    "@types/node": "^6.0.52",
    "clean-css": "^3.4.22",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.0",
    "gulp-pug": "^3.2.0",
    "html-minifier": "^3.2.3",
    "http-server": "^0.9.0",
    "node-sass": "^4.0.0",
    "pug": "^2.0.0-beta6",
    "reflect-metadata": "^0.1.8",
    "rollup": "^0.37.0",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-angular": "^0.4.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-pug": "^0.1.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "^5.0.0-rc.4",
    "rxjs-es": "^5.0.0-beta.12",
    "typescript": "2.0.3",
    "uglify": "^0.1.5",
    "uglifyjs": "^2.4.10",
